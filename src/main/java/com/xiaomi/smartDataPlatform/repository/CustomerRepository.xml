<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.xiaomi.smartDataPlatform.repository.CustomerRepository">
    <select id="findById" parameterType="long" resultType="com.xiaomi.smartDataPlatform.entity.Customer">
        select * from Customer where id = #{id}
    </select>
</mapper>